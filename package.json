{
  "name": "@appatonce/node-sdk",
  "version": "2.0.0",
  "description": "Official Node.js SDK for AppAtOnce - The revolutionary unified backend platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:contract": "jest test/contract/contract.test.ts --config jest.integration.config.js",
    "test:comprehensive": "node tests/comprehensive-sdk-test.js",
    "test:all": "./tests/run-tests.sh",
    "test:ci": "npm run build && npm run test:comprehensive && npm test",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "check:credentials": "node scripts/check-credentials.js",
    "setup:hooks": "./scripts/setup-hooks.sh",
    "prepare-disabled": "npm run build",
    "prepublishOnly": "npm run test:ci"
  },
  "keywords": [
    "appatonce",
    "backend",
    "database",
    "realtime",
    "sdk",
    "typescript",
    "ai",
    "storage",
    "email",
    "logic-server"
  ],
  "author": "AppAtOnce Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "qs": "^6.14.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0",
    "yaml": "^2.8.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appatonce/node-sdk"
  },
  "bugs": {
    "url": "https://github.com/appatonce/node-sdk/issues"
  },
  "homepage": "https://docs.appatonce.com"
}
